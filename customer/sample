  method ZII_SI_CUS_PRO_UP_RES~SI_CUS_PRO_UP_RES.
*** **** INSERT IMPLEMENTATION HERE **** ***
    DATA: status type string.
    UPDATE zscus_profile SET
    COMPANY_NAME = INPUT-MT_cus_pro_up_req-COMPANY_NAME
    ADDRESS = INPUT-MT_cus_pro_up_req-ADDRESS
    NAME = INPUT-MT_cus_pro_up_req-NAME
    MAIL_ID = INPUT-MT_cus_pro_up_req-MAIL_ID
    PHONE_NUMBER = INPUT-MT_cus_pro_up_req-PHONE_NUMBER
    FAX_NUMBER = INPUT-MT_cus_pro_up_req-FAX_NUMBER
    GSTIN_NUMBER = INPUT-MT_cus_pro_up_req-GSTIN_NUMBER
    CITY = INPUT-MT_cus_pro_up_req-CITY
    COUNTRY = INPUT-MT_cus_pro_up_req-COUNTRY
    WHERE CUSTOMER_ID = INPUT-MT_cus_pro_up_req-CUSTOMER_ID.
    status = 1.
    OUTPUT-MT_cus_pro_up_res-STATUS = STATUS.
  endmethod.